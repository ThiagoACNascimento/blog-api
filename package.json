    "docker:compose": "docker compose --env-file .env.development -f src/infrastructure/docker-compose.development.yaml",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "fastify": "5.7.4",
    "fastify-plugin": "5.1.0",
    "pg": "8.18.0"
  },
  "devDependencies": {
    "dotenv": "17.2.3",
    "nodemon": "3.1.11",
    "prettier": "3.8.1"
  }
}
