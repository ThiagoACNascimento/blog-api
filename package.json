{
  "name": "departmental-store",
  "version": "1.0.0",
  "description": "This will be our backend service form departmental stroe on line application",
  "license": "MIT",
  "author": "ThiagoNascimento",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "services:up": "npm run docker:compose -- up -d",
    "services:down": " npm run docker:compose -- down",
